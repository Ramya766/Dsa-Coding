class Main{
    public static void main(String[] args){
        int[] arr={100,2,3,5,7,8,90,99};
        int lar=arr[0];
        int sec=-1;
        for(int i=0;i<arr.length;i++){
            if(arr[i]>lar){
               sec=lar;
               lar=arr[i];
            }
            else if(arr[i]<lar&&sec<arr[i]){
                sec=arr[i];
            }
        }
        System.out.print(lar+" ");
        System.out.print(sec);
       
    }
}
